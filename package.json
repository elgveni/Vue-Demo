    "current-script-polyfill": "^1.0.0",
    "element-ui": "^2.13.0",
    "google-spreadsheet": "^3.1.15",
    "lodash": "^4.17.21",
    "minisearch": "^6.0.1",
    "moment": "^2.29.4",
    "query-string": "^6.11.1",
    "vue": "^2.6.10",
    "vue-custom-element": "^3.3.0",
    "vue-drag-resize": "^1.3.2",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@cypress/vue": "^6.0.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.5.15",
