    "vue-styleguidist": "^4.44.22",
    "vue-template-compiler": "^2.6.10",
    "webpack-node-externals": "^2.5.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
